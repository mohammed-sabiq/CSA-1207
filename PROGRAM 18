LXI H, 8050
MOV A, M
MOV B, A
INX H
MOV A, M
MOV C, A
MOV A, B
CMP C
JC MOVE_C
MOV D, B
JMP BEGIN
MOVE_C: MOV D, C
BEGIN: MOV A, D
MOV E, A
LOOP1: SUB B
JC INCR
JZ STEP2
JMP LOOP1
STEP2: MOV A, D
MOV E, A
LOOP2: SUB C
JC INCR
JZ STORE
JMP LOOP2
INCR: MOV A, D
INR A
MOV D, A
JMP BEGIN
STORE: LXI H, 8052
MOV M, D
HLT